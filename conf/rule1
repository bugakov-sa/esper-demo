insert into FalseAlarm
    select
        a.args('zone') as zone,
        a.time as t1,
        b.time as t2
    from pattern[
        every a=Event -> every b=Event(
            args('zone') = a.args('zone') and
            time - a.time < 5 and
            args('alarm') = 0 and
            a.args('alarm') = 1
        )
    ]